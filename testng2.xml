<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Loan Dept">
<listeners><listener class-name="test1Practice.Listeners"></listener></listeners>
	<parameter name="URL" value="www.medrine.com"></parameter>
	<test name="car loan">
		<classes>
			<class name="test1Practice.CarLoan" />
			<class name="testDemo1" />
		</classes>

	</test>
	<test name="Home loan">
		<classes>
			<class name="test1Practice.HomeLoan">
				<methods>
					<exclude name="homeLoanTypes()"></exclude>
				</methods>
			</class>
		</classes>

	</test>

</suite>
